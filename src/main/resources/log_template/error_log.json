{
  "timestamp": {
    "$resolver": "timestamp",
    "pattern": "yyyy-MM-dd'T'HH:mm:ss.SSSXXX",
    "timeZone": "UTC"
  },
  "trace_id": {
    "$resolver": "runTogetherMDC",
    "key": "traceId"
  },
  "error_code": {
    "$resolver": "runTogetherMDC",
    "key": "errorCode"
  },
  "error_name": {
    "$resolver": "runTogetherMDC",
    "key": "errorName"
  },
  "error_message": {
    "$resolver": "message"
  },
  "message": {
    "$resolver": "runTogetherMDC",
    "key": "message"
  },
  "api_name": {
    "$resolver": "runTogetherMDC",
    "key": "apiName"
  }
}
